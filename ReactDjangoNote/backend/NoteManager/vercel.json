{
    "version": 2,
    "builds": [
        {
            "src": "NoteManager/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "50mb", "runtime": "python3.12.3" }
        },
        {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles_build"
        }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "NoteManager/wsgi.py"
        }
    ]
  }